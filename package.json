    "lint-staged": "^15.2.0",
    "md5": "^2.3.0",
    "next": "^12.3.4",
    "prettier": "^3.1.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.20.0",
